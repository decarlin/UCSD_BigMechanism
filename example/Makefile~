pancrease_wighted_example:
	cut.pl -f 1,3 ~/projects/cell_context_mask/GTEx_pancreas_rpkm_stats.tab \
	| join.pl -o 'NA' diffused_pancreas_weighted.txt - \
	> pancrease_wighted_example.tab

